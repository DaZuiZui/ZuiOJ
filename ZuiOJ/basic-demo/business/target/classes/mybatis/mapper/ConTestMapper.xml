<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dazuizui.business.mapper.ConTestMapper">
    <insert id="insertConTest">
        insert into
            contest
        value(
                null,
                #{name},
                #{startTime},
                #{contestType},
                #{endTime},
                #{createById},
                #{createByName},
                #{createTime},
                #{updateById},
                #{updateByName},
                #{updateTime},
                0,
                0
            )
    </insert>


    <insert id="insertConTestDetailed">
        insert into
            contest_detailed
        value(
            null,
            #{id},
            #{mdText},
            #{htmlText}
        )
    </insert>
</mapper>